.rz_result{
	width:1160px;
	/*height:500px;*/
	/*height:auto;*/
	margin:20px auto;
}
#notice_sheet{
	width:1160px;
	height:500px;
	margin:20px auto;
}
.coop_a{
	width:60px;
}
.ui-jqgrid tr.jqgrow td{
	padding:10px 0;
