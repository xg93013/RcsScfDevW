	width:50px;
	height:50px;
	border-radius: 50%;
	/*background:#333;*/
}
.info_name{
	margin-left: 20px;
}

.person_info:hover b{
	background:url(../images/icon.png) -194px  0 no-repeat;
}
.person_info a b{
    position: absolute;
    right: 10px;
    top: 12px;
    width: 12px;
    height: 16px;
   background:url(../images/icon.png) -167px 0 no-repeat;
    font-size: 0;
    line-height: 0;
    margin:10px 0 0 0px;
}


.exit_link{
	display: block;
	height:30px;
	border-left: 1px solid #eee;
	padding-left: 20px;
	margin-top: 18px;
	margin-left: 4px;
	font-size: 24px;
}




.api-titlebox{
	
}
