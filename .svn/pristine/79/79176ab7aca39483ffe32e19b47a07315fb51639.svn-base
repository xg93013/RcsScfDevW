/*弹出框*/
.black_win{
	width:100%;
	height:100%;
	position: fixed;
	top:0;
	left: 0;
	filter:alpha(opacity=50);   
    -moz-opacity:0.5;   
    -khtml-opacity: 0.5;   
	background: #000000;
	opacity: 0.5;
	z-index: 999;
	display: none;
}
.show_win{
	width:500px;
	height: auto;
	position: fixed;
	top:50%;
	left:50%;
	margin-left: -250px;
	background:#fff;
	z-index: 9999;
	display: none;
}
.down_btn{
	display: block;
	width:390px;
	height:60px;
	margin:60px auto;
	cursor: pointer;
}
.excel_down{
	background:url(../images/icon_3.png) center no-repeat;
}
.txt_down{
	background:url(../images/icon_4.png) center no-repeat;
}

.import_line{
	width:390px;
	overflow: hidden;
	margin:60px auto;
}
.importfile_input{
	width:280px;
	height:42px;
	line-height: 42px;
	outline: none;
	border:1px solid #ccc;
	float: left;
	font-size: 16px;
	padding-left: 10px;
}
.hide_upload{
	position:absolute;
	opacity:0;
	filter:alpha(opacity=0);
	width:90px;
	height:44px;
	right:64px;
	cursor: pointer;
}
.importfile_btn{
	width:90px;
	outline: none;
	border: none;
	height:44px;
	float: left;
	cursor: pointer;
}
.import_btn{
	display: block;
	width:186px;
	margin-right: 10px;
	float: left;
	height:44px;
	border:1px solid #ccc;
	border-radius: 4px;
	background: none;
	cursor: pointer;
}
.import_ok{
	margin-right: 0;
	background:#00b9f2;
	color:#fff;
	border:1px solid #00b9f2;
	
}
.del_info{
	width:100%;
	font-size: 18px;
	text-align: center;
}

.showComWin{
	text-align:center;
	/*padding:40px 0;*/
	font-size:20px;
}
.pwd_win{
	width:400px;
	text-align: center;
	height: auto;
	position: fixed;
	top:50%;
	left:50%;
	margin-left: -250px;
	background:#fff;
	z-index: 9999;
}
.update_pwd_box{
	width:100%;
}
.update_title{
	font-size: 20px;
	color:#333;
	font-weight: bold;
	margin:30px auto 36px;
}
.update_items span{
	display: block;
	width:84px;
	height:35px;
	text-align: left;
	line-height:35px;
	float: left;
	margin-left:10px;
}
.update_items input{
	display: block;
	width:176px;
	height:35px;
	float: left;
	border:none;
	outline: none;
	line-height:35px;
}
.update_bnt{
	width:100%;
	overflow: hidden;
}
.update_items{
	width:300px;
	height:35px;
	margin:16px auto 0;
	overflow: hidden;
	font-size:14px;
	border:1px solid #ccc;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
}
.update_bnt_right{
	width:180px;
	float: right;
	margin:30px 50px 40px 0;
	overflow: hidden;
}
.save_update{
	display: block;
	width:300px;
	height:40px;
	line-height:40px;
	border:none;
	outline:none;
	background:#4B7FD5;
	color:#fff;
	margin:10px auto 40px;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	cursor: pointer;
}
.update_pwd_info{
	margin:20px 0 0 48px;
	overflow: hidden;
	float: left;
}
.update_pwd_info img{
	float: left;
}
.update_pwd_info span{
	float: left;
	margin:2px 0 0 4px;
	color:red;
}
.update_msg{
	width:30px;
	height:30px;
	float: left;
}
.right_msg{
	background:url(../images/icon.png) 0px -100px no-repeat;
}
.err_msg{
	background:url(../images/icon.png) 0px -126px no-repeat;
}
.red_border{
	border:1px solid #DE424D;
}