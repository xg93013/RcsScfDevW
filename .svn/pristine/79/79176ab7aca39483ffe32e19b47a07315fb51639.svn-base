.context-box{
	width: 100%;
	height: 100%;
	display: none;
}
#demo_property h2{
	color : #fff;
	background : #7f8fb1;
	margin : 10px;
	text-indent : 10px;
}
#demo_property table{
	border-top : 1px solid #999;
	border-left : 1px solid #999;
	width : 90%;
	position : relative;
	margin : 0 0 50px 10px;
}
#demo_property th{
	font-weight : bold;
}
#demo_property th,#demo_property td{
	border-right : 1px solid #999;
	border-bottom : 1px solid #999;
	padding : 5px;
	text-align : center;
}


.hide_nlist{
	position: absolute;
	top:55px;
	right:0px;
	width:137px;
	/*height:323px;*/
	/*padding-top:10px;*/
	z-index:9999;
	display: none;
	background:#fff;
	border-radius: 4px;
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	-webkit-box-shadow:0 0 10px #ccc;
	-moz-box-shadow:0 0 10px #ccc;
	box-shadow:0 0 10px #ccc;
	/*background:url(../images/pop_bg_03.png) center no-repeat;*/
}
.hide_nlist ul li{
	width:100%;
	height:45px;
	margin-top:1px;
	font-size:14px;
	line-height: 45px;
	text-align: center;
}
.hide_nlist ul li a{
	font-size: 16px;
}

.tab_list{
	overflow: hidden;
	margin:15px auto 6px;
}
.tab_list li{
	float: left;
	font-size: 16px;
	color:#666;
	margin-right: 10px;
}

/*footer*/
.footer{
	width:100%;
	text-align: center;
	color:#fff;
	padding:40px 0;
	font-size: 16px;
	line-height: 30px;
	/*height:200px;*/
	background: #232735;
}

.main_content{
	background: #f5f5f5;
	padding-bottom: 50px;
}

/*5步流程*/
.step_content{
	background:#fff;
	overflow: hidden;
	margin-top:10px;
}
.rz_nav{
	width:100%;
	height:116px;
	position: relative;
}
.rz_nav a{
	position:absolute;
	display: block;
	height:116px;
	overflow: hidden;
	font-size: 20px;
	color:#fff;
}
.on_state, .off_state{
	margin-left: 36px;
}
.on_state{
	font-size: 34px;
	display: none;
	margin-top:40px;
	margin-left: 50px;
}

.rz_nav ul li a .font16{
	font-size: 16px;
	color:#ccc;
}
.off_state{
	margin-top:25px;
}
.off_state p{
	line-height: 32px;
}
.msg_num{
	position: relative;
}
.msg_num span{
	display: inline-block;
	width:20px;
	height:20px;
	padding:2px;
	border-radius: 50%;
	text-align: center;
	line-height: 20px;
	position: absolute;
	right: -40px;
	top: -10px;
	background:#fd7138;
	color:#fff;
	font-size: 12px;
}
.tab_01_over .on_state{
	margin-left: 30px;
}
