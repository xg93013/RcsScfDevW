.showInfo{
	width:1160px;	
	margin: 20px auto 0px auto;
}
.showInfo_list{
	height:56px;
	line-height: 56px;
	position: relative;
}
.showInfo_list a{
	
	display: inline-block;
	text-align: center;
	position: absolute;
	
}
.showInfo_list a.list_1,.showInfo_list a.list_2,.showInfo_list a.list_3{
	color: #fff;
	font-size: 20px;
}
.showInfo_list a.list_1_over,.showInfo_list a.list_2_over,.showInfo_list a.list_3_over,.showInfo_list a.list_1_over_over,.showInfo_list a.list_2_over_over,.showInfo_list a.list_3_over_over{
	color: #333;
	font-size: 16px;
}
.showInfo_list a.list_1{
	background: url(../images/progress_bg_34.png) no-repeat;
}
.showInfo_list a.list_1_over{
	background: url(../images/progress_bg_31.png) no-repeat;
}
.showInfo_list a.list_1_over_over{
	background: url(../images/progress_bg_36.png) no-repeat;
}
.showInfo_list a.list_2{
	background: url(../images/progress_bg_45.png) no-repeat;	
}
.showInfo_list a.list_2_over{
	background: url(../images/progress_bg_42.png) no-repeat;
}
.showInfo_list a.list_2_over_over{
	background: url(../images/progress_bg_47.png) no-repeat;
}

.showInfo_list a.list_3{
	background: url(../images/progress_bg_54.png) no-repeat;	
}
.showInfo_list a.list_3_over{
	background: url(../images/progress_bg_52.png) no-repeat;
}
.showInfo_list a.list_3_over_over{
	background: url(../images/progress_bg_56.png) no-repeat;
}
.showInfo_list_span{
	font-size: 16px;
	color: #333;
}
.showInfo_list_span span{
	display: inline-block;
	width:188px;
	text-align: center;
	line-height: 48px;
}
/*融资信息流程css结束*/
/*选择框的样式开始*/
.chioce{
	position: absolute;
	z-index: 101;
}
.pos_supplier{
	margin-top: 64px;
    margin-left: 221px;
}
.chioce_list ul{
	border:1px solid #e2e2e2;
	border-radius: 5px;
	background-color: #fff;
}
.chioce_list ul li{
	list-style: none;
	display: block;
	width:221px;
	height: 45px;
	line-height: 45px;
	text-align: left;
	color: #333;
	font-size: 16px;
	border-bottom: 1px solid #BABABA;
	padding: 0px 10px;
}
.chioce_list ul li .serach_ipt{
	border: 0px;
    font-size: 16px;
    color: #999;
    margin-left: 5px;
    width: 190px;
    height: 30px;
    line-height: 30px;
}
.box_shadow{  
  -webkit-box-shadow:0 0 10px #ddd;  
  -moz-box-shadow:0 0 10px #ddd;  
  box-shadow:0 0 10px #ddd;  
}  
.triangle{
	background: url(../images/chioce_bg1.png) no-repeat 0px 0px;
    width: 25px;
    height: 11px;
    display: inline-block;
    position: absolute;
    left: 100px;
    top: -9px;
}
.chioce_list ul li span.all_chioce{
	display: inline-block;
	vertical-align: text-bottom;
}
.chioce_list ul li b,.chioce_list ul li span.serach_icon{
	
    display: inline-block; 
    text-align: center;
    vertical-align: text-bottom;
    margin-right: 5px;
}
.chioce_list ul li .b_1,.chioce_list ul li .b_2{	
	width: 21px;
    height: 21px; 
    line-height: 21px;
}
.chioce_list ul li .b_1{
	background: url(../images/icon.png) no-repeat -388px 0px; 
	
}
.chioce_list ul li .b_2{
	background: url(../images/icon.png) no-repeat -412px 0px;
}
.chioce_list ul li span.serach_icon{
	background: url(../images/icon.png) no-repeat -432px 0px;
	width: 23px;
    height: 23px;
    line-height: 23px;
    margin-right: 0px;
    
}
/*选择框的样式结束*/
/*按钮样式*/
.btn{
	height:40px;
	line-height: 40px;
	text-align: center;
	border-radius: 5px;
	display: inline-block;
	vertical-align: middle;
}
.btn_div{
	color: #3b61b9;
	font-size: 16px;
	line-height: 25px;
	cursor: pointer;
}
.btn_1{
	border-radius: 5px;
	border: 1px solid #bababa;
	color: #333;
	display: inline-block;
	text-align: center;
}
.btn_2{
	background-color: #00b9f2;
	border-radius: 5px;
	border: 1px solid #00b9f2;
	color: #fff;
	display: inline-block;
	text-align: center;
}
.btn_3{
	background-color: #fd7138;
	border-radius: 5px;
	border: 1px solid #fd7138;
	color: #fff;
	display: inline-block;
	text-align: center;
}

