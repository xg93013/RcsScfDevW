    showCount: function (data) {
        $(".todo-count").text(data.count);
    },
    checkValid: function () {

        return true;
    },
    getData: function () {
        var data = {};
        //TODO:
        return data;
    },
    submit: function () {
        if (!this.checkValid()) {
            return;
        }
        EUI.Stor({
            url: "",
            params: this.getData(),
            success: function (response) {
                g.showCount(response.data);
            }
        });
    }
