.overlay {   
    position: fixed;   
    top: 0px;   
    left: 0px;   
    z-index: 10001;     
    filter:alpha(opacity=60);   
    background-color: #777;   
    opacity: 0.5;   
    -moz-opacity: 0.5;   
    width:100%;
    height:100%;
}  
.loading_tip {   
    z-index: 10002;
    position: fixed;
    width: 500px;
    height: 295px;
    margin-top: -150px;
    margin-left: -250px;
