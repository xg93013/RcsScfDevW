function  getRecMoney() {
    EUI.Store({
        url: _ctxPath+"/avf/accountRPInfo/accountRPInfoList.json",
        params: {
            query:JSON.stringify({
                "businessDealStatus":"1050",
                "companyCode":$('#getCompanyCode').val(),
